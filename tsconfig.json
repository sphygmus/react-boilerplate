{
	"compilerOptions": {
		"target": "ES5",
		"module": "CommonJS",
		"moduleResolution": "node",
		"lib": [
			"DOM",
			"ESNext"
		],
		"jsx": "react-jsx",
		"noEmit": true,
		"isolatedModules": true,
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true
	},
	"include": [
		"src/**/*"
	]
}